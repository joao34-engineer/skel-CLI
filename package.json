{
  "name": "software-skeleton-cli",
  "version": "2.0.0",
  "description": "Software Skeleton CLI - Factory automation for primitives",
  "type": "module",
  "bin": {
    "skel": "./bin/run.js"
  },
  "oclif": {
    "commands": "./dist/src/commands",
    "bin": "skel",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "factory": "ts-node scripts/factory.ts",
    "build": "tsc",
    "test": "vitest"
  },
  "dependencies": {
    "@oclif/core": "^3.27.0",
    "archiver": "^7.0.1",
    "chalk": "^5.6.2",
    "execa": "^9.5.2",
    "fs-extra": "^11.3.2",
    "inquirer": "^8.2.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.6",
    "@stryker-mutator/core": "^9.3.0",
    "@stryker-mutator/typescript-checker": "^9.3.0",
    "@stryker-mutator/vitest-runner": "^9.3.0",
    "@types/archiver": "^7.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^24",
    "@vitest/ui": "^2.0.4",
    "fast-check": "^4.3.0",
    "ts-node": "^10",
    "typescript": "^5",
    "vitest": "^2.0.4"
  },
  "volta": {
    "node": "24.8.0",
    "npm": "10.8.3"
  }
}
